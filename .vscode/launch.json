{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug start:dev",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:dev"],
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    }
  ]
}
